<div class='grid-block'>
  <div class='grid-block__header'>
    <h4 class='grid-block__header_title'>{{title}}</h4>
  </div>
  <div class='grid-block__content'>
    {{#each items}}
      <div class='grid-block__element'>
        {{! <div class=""></div> }}
        <img
          id='movie-image-{{this.id}}'
          class='grid-block__element_image'
          src={{this.albumImage}}
          alt='card'
        />
        <div class='grid-block__element_desc'>
          <span class='grid-block__element_desc_title'>{{this.title}}</span>
          <div class='grid-block__element_desc_stats'>
            <span>{{this.country}}</span>
            <span>{{this.releaseDate}}</span>

            <span>{{this.rating}}</span>

          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>